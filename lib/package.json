{
  "name": "@relewise/relewise-client",
  "version": "0.0.1-alpha.1",
  "description": "Relewise recommendation engine JS/TS client API https://relewise.com",
  "author": "Rasmus Ã˜stergaard",
  "homepage": "https://relewise.com",
  "license": "ISC",
  "keywords": [
    "relewise",
    "recommendations",
    "personalization"
  ],
  "main": "dist/cjs/relewise-client.js",
  "module": "dist/esm/relewise-client.js",
  "browser": "dist/browser/relewise-client.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs",
    "gen-api": "swagger-typescript-api -p swagger.json -o src/models -t ./api-templates --modular --clean-output --no-client --enum-names-as-values"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.28.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.1",
    "swagger-typescript-api": "^9.3.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
